<import src="../stars/stars.wxml"/>
<template name="movie-template">
<view class="movie-container" catchtap="ondetails" data-id="{{id}}">
  <image class="movie-img" src="{{images.large}}"></image>
  <text class="movie-title">{{changetitel}}</text>
<template is="stars" data="{{rating,stars:stars}}"></template>


</view>
        

</template>
